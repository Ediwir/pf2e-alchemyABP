{"name":"Alchemist's Fire (Greater)","type":"weapon","img":"systems/pf2e/icons/equipment/alchemical-items/alchemical-bombs/alchemists-fire.webp","data":{"description":{"value":"<p><strong>Activate</strong> <span class=\"pf2-icon\">A</span> Strike</p>\n<p>Alchemist's fire is a combination of volatile liquids that ignite when exposed to air. Alchemist's fire deals 3d8 fire damage, 3 persistent fire damage, and 3 fire splash damage. You gain a +2 item bonus to attack rolls.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["hb_ABP","alchemical","bomb","consumable","fire","splash"],"rarity":"common","custom":""},"rules":[{"key":"Note","outcome":["success"],"selector":"{item|_id}-damage","text":"<p class='compact-text'>@Localize[PF2E.BombNotes.AlchemistsFire.Greater.success]</p>"},{"key":"Note","outcome":["criticalSuccess"],"selector":"{item|_id}-damage","text":"<p class='compact-text'>@Localize[PF2E.BombNotes.AlchemistsFire.Greater.criticalSuccess]</p>"}],"slug":"alchemists-fire-greater","schema":{"version":0.743,"lastMigration":null},"quantity":1,"baseItem":"alchemical-bomb","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"L"},"equippedBulk":{"value":""},"price":{"value":"250 gp"},"equipped":{"carryType":"worn","invested":null,"handsHeld":0},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"size":"med","identification":{"status":"identified","unidentified":{"name":"","img":"","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"level":{"value":11},"category":"martial","group":"bomb","bonus":{"value":2},"damage":{"value":"","dice":1,"die":"d8","damageType":"fire"},"bonusDamage":{"value":0},"splashDamage":{"value":3},"range":20,"reload":{"value":""},"MAP":{"value":""},"potencyRune":{"value":null},"strikingRune":{"value":""},"specific":{"value":false},"propertyRune1":{"value":""},"propertyRune2":{"va