{"name":"Blight Bomb (Lesser)","type":"weapon","img":"systems/pf2e/icons/equipment/alchemical-items/alchemical-bombs/blight-bomb.webp","data":{"description":{"value":"<p><strong>Usage</strong> held in 1 hand</p>\n<p><strong>Activate</strong> <span class=\"pf2-icon\">A</span> Strike</p>\n<p>Blight bombs contain volatile toxic chemicals that rot flesh. A blight bomb deals 1d6 poison damage, 1d4 persistent poison damage, and 1 poison splash damage.</p>"},"source":{"value":"Pathfinder Advanced Player's Guide"},"traits":{"value":["hb_ABP","alchemical","bomb","consumable","poison","splash"],"rarity":"common","custom":""},"rules":[{"key":"Note","outcome":["success"],"selector":"{item|_id}-damage","text":"<p class='compact-text'>@Localize[PF2E.BombNotes.BlightBomb.Lesser.success]</p>"},{"key":"Note","outcome":["criticalSuccess"],"selector":"{item|_id}-damage","text":"<p class='compact-text'>@Localize[PF2E.BombNotes.BlightBomb.Lesser.criticalSuccess]</p>"}],"slug":"blight-bomb-lesser","schema":{"version":0.743,"lastMigration":null},"quantity":1,"baseItem":"alchemical-bomb","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"L"},"equippedBulk":{"value":""},"price":{"value":"3 gp"},"equipped":{"carryType":"worn","invested":null,"handsHeld":0},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"size":"med","identification":{"status":"identified","unidentified":{"name":"","img":"","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"level":{"value":1},"category":"martial","group":"bomb","bonus":{"value":null},"damage":{"value":"","dice":1,"die":"d6","damageType":"poison"},"bonusDamage":{"value":null},"splashDamage":{"value":1},"range":20,"reload":{"value":""},"MAP":{"value":""},"potencyRune":{"value":0},"strikingRune":{"value":""},"specific":{"value":false},"propertyRune1":{"value":null},"propertyRune2":{"value":null},"propertyRune3