{"name":"Acid Flask (Greater)","type":"weapon","img":"systems/pf2e/icons/equipment/alchemical-items/alchemical-bombs/acid-flask.webp","data":{"description":{"value":"<p><strong>Activate</strong> <span class=\"pf2-icon\">A</span> Strike</p>\n<p>This flask filled with corrosive acid deals 1 acid damage, 3d6 persistent acid damage, and 3 acid splash damage. You gain a +2 item bonus to attack rolls.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["acid","alchemical","bomb","consumable","splash"],"rarity":"common","custom":""},"rules":[{"key":"Note","outcome":["success"],"selector":"{item|_id}-damage","text":"<p class='compact-text'>@Localize[PF2E.BombNotes.AcidFlask.Greater.success]</p>"},{"key":"Note","outcome":["criticalSuccess"],"selector":"{item|_id}-damage","text":"<p class='compact-text'>@Localize[PF2E.BombNotes.AcidFlask.Greater.criticalSuccess]</p>"}],"slug":"acid-flask-greater","schema":{"version":0.743,"lastMigration":null},"quantity":1,"baseItem":"alchemical-bomb","hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"L"},"equippedBulk":{"value":""},"price":{"value":"250 gp"},"equipped":{"carryType":"worn","invested":null,"handsHeld":0},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"size":"med","identification":{"status":"identified","unidentified":{"name":"","img":"","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"level":{"value":11},"category":"martial","group":"bomb","bonus":{"value":2},"damage":{"value":"","dice":1,"die":"","damageType":"acid"},"bonusDamage":{"value":0},"splashDamage":{"value":3},"range":20,"reload":{"value":""},"MAP":{"value":""},"potencyRune":{"value":null},"strikingRune":{"value":""},"specific":{"value":false},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"property1":{"value":"","dice":null,